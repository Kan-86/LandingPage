@model AcmeCorporation.Areas.Identity.Data.UserProfile



@{
    ViewData["Title"] = "DrawForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Draw Form</h1>

<h4>UserProfile</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        
        @using (Html.BeginForm("Login"))
        {
            <input class="form-control" type="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" name="Email" />
            <input class="form-control" type="text" name="SN" id="Sn" placeholder="Insert SerialNumber" />
            <input class="btn btn-primary" type="submit" value="Submit" />
        }
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
